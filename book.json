{
  "title": "Java / Android 笔试、面试 知识整理",
  "author": "HadYang/咕咚",
  "links": {
    "sidebar": {
        "我的狗窝": "https://gudong.name"
    }
  },
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "-sharing",
    "disqus",
    "github-buttons",
    "editlink",
    "ga",
    "prism",
    "-highlight",
    "anchor-navigation-ex",
    "page-footer-ex",
    "splitter",
    "todo",
    "copy-code-button",
    "expandable-chapters-small",
    "mathjax@https://github.com/OrgVue/gitbook-plugin-mathjax.git#speech-fix"
  ],
  "pluginsConfig": {
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord" : ["Gitbook Use"]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "disqus": {
      "shortName": "gudong-name"
    },
    "github": {
      "url": "https://github.com/maoruibin"
    },
    "github-buttons": {
      "buttons": [{
        "user": "maoruibin",
        "repo": "interview",
        "type": "star",
        "size": "small",
        "count": true
      }]
    },
    "editlink": {
      "base": "https://github.com/maoruibin/interview/tree/master",
      "label": "Edit",
      "multilingual": false
    },
    "ga": {
      "token": "UA-131910384-3"
    },
    "prism": {
      "ignore": [
        "mermaid",
        "eval-js"
      ],
      "css": [
        "prismjs/themes/prism-solarizedlight.css"
      ]
    },
    "page-footer-ex": {
      "copyright": "使用[知识共享 署名-相同方式共享 4.0协议](https://creativecommons.org/licenses/by-sa/4.0/)发布",
      "markdown": true,
      "update_label": "此页面修订于：",
      "update_format": "YYYY-MM-DD HH:mm:ss"
    },
    "mathjax":{
      "forceSVG": false
    }
  }
}
